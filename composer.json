{
    "name": "hauer_heinrich/hh_indexed_search",
    "type": "typo3-cms-extension",
    "description": "Currently fixes https://forge.typo3.org/issues/95238.",
    "authors": [
        {
            "name": "Christian Hackl",
            "email": "info@hauer-heinrich.de",
            "homepage": "http://www.hauer-heinrich.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Hauer-Heinrich/hh_indexed_search/issues",
        "source": "https://github.com/Hauer-Heinrich/hh_indexed_search",
        "docs": "https://github.com/Hauer-Heinrich/hh_indexed_search/blob/master/README.md"
    },
    "version": "0.1.0",
    "license": ["GPL-2.0-or-later"],
    "keywords": [
        "TYPO3 CMS",
        "extension",
        "hauer-heinrich"
    ],
    "replace": {
        "hauer-heinrich/hh_indexed_search": "self.version",
        "typo3-ter/hh_indexed_search": "self.version"
    },
    "require": {
        "typo3/cms-core": "^10.2.0",
        "typo3/cms-fluid": "^10.2.0",
        "typo3/cms-frontend": "^10.2.0",
        "typo3/cms-fluid-styled-content": "^10.2.0"
    },
    "autoload": {
        "psr-4": {
            "HauerHeinrich\\HhIndexedSearch\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web",
            "extension-key": "hh_indexed_search"
        }
    }
}
